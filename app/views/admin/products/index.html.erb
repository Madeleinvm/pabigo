        <div id="blue">
        <div class="row">
                        <div class="col-lg-12">
                            <h1 class="page-header">Dar donacion de Productos</h1>
                        </div>
                        <!-- /.col-lg-12 -->
                    </div>
                    <!-- /.row -->
                    <div class="row">
                    <div class="row">
                <div class="col-lg-12">
                    <section class="panel">
                    <header class="panel-heading">
                    <div class="container-fluid">
                        <div class="nav row" id="top_menu">

                            <div class="col-md-4"><h4>Productos existentes</h4></div>

                            <div class="col-md-6">
                                    <div class="col-lg-6">
                                       <div class="col-md-4"><h4>Donar</h4></div>
                                    </div>
                                    <div class="col-lg-3">
                                       <div class="col-md-4"><h4>Donar </h4></div>
                                    </div>
                            </div>

                            <div class="col-md-2">
                                <a class="btn btn-info" href="<%= new_admin_product_path %>"><i class="fa fa-plus-square" aria-hidden="true"></i></a>
                            </div>
                        </div>
                        </div>
                    </header>

                        <table class="table table-striped table-advance table-hover">
                            <tbody>
                                <tr>
                                    <th><i class="fa fa-photo blue"></i> Foto del Producto</th>
                                    <th><i class="fa fa-user blue"></i> Nombre</th>
                                    <th><i class="fa fa-file-text blue"></i> Descripcion</th>
                                    <th><i class="fa fa-hand-o-up blue "></i> Tipo de Producto</th>
                                    <th><i class="fa fa-hand-o-up blue "></i> Cantidad de Producto</th>
                                </tr>
                                <% @products.each do |product| %>
                                <tr>
                                    <td>

                                            <ul class="list-inline">
                                                <% unless product.images.first == nil %>
                                                    <img  src="<%= product.images.first.picture.url %>" width="150" height="110" >
                                                <% else %>
                                                    <img  src="/images/productomascota.jpg" width="150" height="110">
                                                <% end %>

                                            </ul>
                                    </td>
                                    <td><%= product.name %></td>
                                    <td><%= product.description.html_safe %></td>
                                    <td><%= product.product_type %></td>
                                    <td><%= product.qty %></td>
                                    <td>
                                        <div>
                                            <a class="btn btn-warning" href="<%= admin_productshow_path(product) %>"><i class="fa fa-search" aria-hidden="true"></i></a>
                                            <a href="<%= admin_productsedit_path(product)%>"><button class="btn btn-success"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button></a>
                                            <%= link_to '', admin_productsdeletes_path(product), method: :delete, class:"fa fa-trash-o btn btn-danger", data: { confirm: "¿Esta seguro que desea eliminar la donación?"}  %>
                                        </div>
                                    </td>
                                </tr>
                                <% end %>
                            </tbody>
                        </table>

                    </section>
                </div>
                </div>
            </div>
        </div>
